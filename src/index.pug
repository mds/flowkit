          .fk-carousel-container
            .fk-arrow-desc-container
              div
                p.fk-arrow-desc tap right arrow
              div
                p.fk-label.fk-label--right
                  span.fk-current 1
                  |  of 11
            .fk-arrow-container.fk-arrow-is-pulsing
              .fk-arrow.fk-arrow--tap.fk-arrow--default(data-pos='pos1')
                img.svg(src='assets/img/01-tap-right.svg')
                img.svg.is-hidden(src='assets/img/02-tap-right-curve-down.svg')
                img.svg.is-hidden(src='assets/img/03-tap-right-curve-down-hook.svg')
                img.svg.is-hidden(src='assets/img/04-tap-right-curve-down-u-turn.svg')
                img.svg.is-hidden(src='assets/img/05-tap-right-curve-up.svg')
                img.svg.is-hidden(src='assets/img/06-tap-right-curve-up-hook.svg')
                img.svg.is-hidden(src='assets/img/07-tap-right-curve-up-u-turn.svg')
                img.svg.is-hidden(src='assets/img/08-tap-right-snake-up.svg')
                img.svg.is-hidden(src='assets/img/09-tap-right-snake-up-hook.svg')
                img.svg.is-hidden(src='assets/img/10-tap-right-snake-down.svg')
                img.svg.is-hidden(src='assets/img/11-tap-right-snake-down-hook.svg')

            .fk-cycle-control-container
              .fk-cycle-controls
                ul
                  li: a.fk-cycle-prev
                      span Previous
                      img.svg(src='assets/img/arrow.svg')
                  li: a.fk-cycle-next
                      span Next
                      img.svg(src='assets/img/arrow.svg')
            .fk-bottom-controls
              div
                ul.fk-controls-type
                  li: a.is-active(data-type='tap') Tap
                  li: a(data-type='flow') Flow
              div
                ul.fk-controls-direction
                  li: a.is-active(data-direction='right') ‚Üí
                  li: a(data-direction='down') ‚Üì
                  li: a(data-direction='left') ‚Üê
                  li: a(data-direction='up') ‚Üë
              div
                ul.fk-controls-style
                  li: a.is-active(data-style='default') Default
                  li: a(data-style='alt') Alt

